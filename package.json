{
  "name": "print-management",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "bin": "bin/www.js",
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www.js",
    "dev": "nodemon ./bin/www.js",
    "lint": "eslint .",
    "setup": "node ./setup.js",
    "clean": "rimraf dist build",
    "obfuscate": "javascript-obfuscator ./ --output ./dist --exclude node_modules,dist",
    "build": "npm run clean && npm run obfuscate && nexe --input ./dist/bin/www.js --output ./build/print-management-linux --target linux-x64-10.0.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "dotenv-safe": "^9.1.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "net-snmp": "^3.18.2",
    "node-schedule": "^2.1.1",
    "pdf-lib": "^1.17.1",
    "pg": "^8.13.1",
    "pm2": "^5.4.3",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "boxednode": "^2.4.4",
    "eslint": "^9.18.0",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^15.14.0",
    "javascript-obfuscator": "^4.1.1",
    "nexe": "^4.0.0-rc.6",
    "node-packer": "^0.1.7",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*",
      ".env.example"
    ],
    "scripts": [
      "bin/www.js"
    ],
    "targets": [
      "node20-linux-x64"
    ],
    "outputPath": "build"
  }
}
